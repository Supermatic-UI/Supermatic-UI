  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "exclude": ["src/**/__tests__/*"],
  "target": "ESNext",
  "compilerOptions": {
    "composite": true,
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "noUnusedLocals":false
