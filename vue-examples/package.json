{
  "name": "@supermatic-ui/vue-examples",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "clean": "rimraf dist",
    "clean:node_modules": "rimraf node_modules",
    "update-schema": "copyfiles --flat ../core/dist/layoutMetadataSchema.json ./src/assets"
  },
  "dependencies": {
    "@monaco-editor/loader": "^1.3.3",
    "@supermatic-ui/core": "link:../core",
    "@supermatic-ui/vue-components": "link:../vue-components",
    "@supermatic-ui/vue-core": "link:../vue-core",
    "rimraf": "^4.4.1",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "copyfiles": "^2.4.1",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vite-plugin-plain-text": "^1.4.2",
    "vue-tsc": "^1.2.0"
  }
}
